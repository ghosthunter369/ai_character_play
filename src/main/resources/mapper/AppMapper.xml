<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.character.mapper.AppMapper">

    <resultMap id="BaseResultMap" type="com.character.model.entity.App">
            <id property="appId" column="app_id" />
            <result property="description" column="description" />
            <result property="appName" column="app_name" />
            <result property="initPrompt" column="init_prompt" />
            <result property="cover" column="cover" />
            <result property="userId" column="user_id" />
            <result property="prologue" column="prologue" />
            <result property="priority" column="priority" />
            <result property="createTime" column="create_time" />
            <result property="editTime" column="edit_time" />
            <result property="isDelete" column="is_delete" />
    </resultMap>

    <sql id="Base_Column_List">
        app_id,description,app_name,init_prompt,cover,user_id,prologue,priority,edit_time
        create_time,is_delete
    </sql>
</mapper>
